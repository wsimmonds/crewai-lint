{
  "name": "crewai-lint",
  "displayName": "CrewAI Lint",
  "description": "Basic linting for agents.yaml and tasks.yaml to help prevent common errors",
  "version": "0.102.1",
  "publisher": "williamsimmonds",
  "repository": {
    "type": "git",
    "url": "https://github.com/wsimmonds/crewai-lint"
  },
  "bundleDependencies": [
    "js-yaml"
  ],
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "yaml",
        "filenames": [
          "agents.yaml",
          "tasks.yaml"
        ]
      }
    ],
    "commands": [
      {
        "command": "crewai-lint.setVersion",
        "title": "CrewAI: Set Schema Version"
      }
    ]
  },
  "files": [
    "out/**/*.js",
    "out/schemas/**/*.js"
  ],
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "tsc -p ./",
    "bundle-deps": "node bundle-dependencies.js",
    "package": "pnpm run compile && pnpm run bundle-deps && echo \"y\" | pnpm dlx vsce package --no-dependencies",
    "test": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "~20.17.22",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^2.23.0",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.0",
    "glob": "^11.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}
